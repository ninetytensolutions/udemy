<p id="notice"><%= notice %></p>

<h1>Listing Ninety Ten Bookings</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Time</th>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Phonenumber</th>
      <th>Noofguests</th>
      <th>Comments</th>
      <th>Status</th>
      <th>Username</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ninety_ten_bookings.each do |ninety_ten_booking| %>
      <tr>
        <td><%= ninety_ten_booking.date %></td>
        <td><%= ninety_ten_booking.time %></td>
        <td><%= ninety_ten_booking.firstName %></td>
        <td><%= ninety_ten_booking.lastName %></td>
        <td><%= ninety_ten_booking.phoneNumber %></td>
        <td><%= ninety_ten_booking.noOfGuests %></td>
        <td><%= ninety_ten_booking.comments %></td>
        <td><%= ninety_ten_booking.status %></td>
        <td><%= ninety_ten_booking.username %></td>
        <td><%= link_to 'Show', ninety_ten_booking %></td>
        <td><%= link_to 'Edit', edit_ninety_ten_booking_path(ninety_ten_booking) %></td>
        <td><%= link_to 'Destroy', ninety_ten_booking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ninety ten booking', new_ninety_ten_booking_path %>
